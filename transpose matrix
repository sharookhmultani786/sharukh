#include<stdio.h>
int main()
{
	int a[10][10],transpose[10][10],n,i,j;
	printf("enter value of n(N<=10):");
	scanf("%d",&n);
	printf("enter value of matrix a:\n");
	for(i=0;i<n;i++)
	{
		for(j=0;j<n;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}
		for(i=0;i<n;i++)
		{
			for(j=0;j<n;j++)
			{
				transpose[i][j]=a[j][i];
				printf("%d\t",transpose[i][j]);
			}
			printf("\n");
		}
	
}
