#include <stdio.h>
int main()
{
  int length, breadth, area;
  
  printf ("enter the value of length \n");
  scanf ("%d", &length);
  
  printf ("enter the value of breadth \n");
  scanf ("%d",&breadth);
  
  area = length*breadth;
  
  printf ("the area of rectangle is %d", area);
  
  return 0;
}
